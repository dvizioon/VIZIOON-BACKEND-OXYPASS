{
  "name": "api-jwt-express",
  "version": "1.0.0",
  "description": "API Express com autenticação Para Mudar Senha Moodle",
  "main": "./src/server.js",
  "scripts": {
    "// === EXECUTAR VERSÃO BUILDADA ===": "",
    "start": "node dist/server.bundle.js",
    "// === DATABASE ===": "",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:migrate": "node ./config/database/sync.js",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "node ./config/database/seed.js",
    "setup": "npm run db:create && npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0",
    "sequelize-auto-migrations": "^1.0.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^11.1.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "glob": "^10.4.5",
    "javascript-obfuscator": "^4.1.1",
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.2",
    "webpack": "^5.101.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0",
    "webpack-obfuscator": "^3.5.1"
  },
  "keywords": [
    "api",
    "express",
    "jwt",
    "authentication",
    "sequelize",
    "postgresql"
  ],
  "author": "",
  "license": "MIT"
}
